program

    int n;
    int reverse;
    bool result;
    int temp;
    
begin

	n	=1234321;
    temp=   n;
    
    while(temp!=0)
    {
        reverse = reverse *10;
        reverse= reverse+temp%10;
        temp = temp/10;
    }
    
    if (n == reverse) {
        result = true;
    } else {
        result = false;
    }
    
end
